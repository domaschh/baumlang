let add1 = |a| {
    a + 1
}
let map = |f, lst| {
    if len(lst) == 0 {
        []
    } else {
        concat(f(head(lst)), map(f, tail(lst)))
    }
}

let reduce = |f, lst, initial| {
    if len(lst) == 0 {
        initial
    } else {
        reduce(f, tail(lst), f(initial, head(lst)))
    }
}

let add = |a, b| {
    a + b
}

let sum = |lst| {
    reduce(add, lst, 0)
}

print(map(add1, [1,2])) # should print [2,3]
print(sum([1, 2, 3, 4, 5]))  # Should print 15
